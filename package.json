{
  "name": "updtr",
  "version": "1.0.0",
  "description": "Update outdated npm modules with zero painâ„¢",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --recursive -R spec",
    "test:watch": "jest --watch",
    "posttest": "npm run lint",
    "coverage": "istanbul cover _mocha -- -R spec",
    "lint": "eslint bin lib test",
    "fixtures:ensure": "babel-node ./test/helpers/setupFixtures.js",
    "fixtures:rebuild": "babel-node ./test/helpers/cleanupFixtures.js && npm run fixtures:ensure"
  },
  "author": "peerigon",
  "license": "Unlicense",
  "bin": {
    "updtr": "./bin/updtr"
  },
  "dependencies": {
    "ansi-escape-sequences": "^3.0.0",
    "async": "^2.2.0",
    "chalk": "^1.1.3",
    "cli-cursor": "^2.1.0",
    "cli-spinner": "^0.2.6",
    "cli-spinners": "^1.0.0",
    "cli-truncate": "^1.0.0",
    "colors": "^1.1.2",
    "commander": "^2.8.1",
    "ora": "^1.2.0",
    "pify": "^2.3.0",
    "semver": "^5.3.0",
    "string-width": "^2.0.0",
    "strip-ansi": "^3.0.1",
    "unicons": "0.0.3",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "chai": "^3.3.0",
    "clone": "^2.1.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-peerigon": "^10.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsdoc": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^19.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "pify": "^2.3.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "temp": "^0.8.3"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peerigon/updtr.git"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
